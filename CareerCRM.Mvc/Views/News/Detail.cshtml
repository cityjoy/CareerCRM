@using CareerCRM.Repository.Domain;
@model News
<div class="layui-form">
    <table class="layui-table">
        <tbody>
            <tr>
                <td>名称</td>
                <td>@Html.Raw(Model.Name)</td>

            </tr>
            <tr>
                <td>内容</td>
                <td>@Html.Raw(Model.Content)</td>

            </tr>
            <tr>
                <td>时间</td>
                <td>@Html.Raw(Model.CreateTime)</td>

            </tr>
        </tbody>
    </table>
</div>
